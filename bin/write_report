#!/usr/bin/env python

import sys

from pylprotpredictor.write_report import write_report

if not snakemake:
    sys.exit("This script can only be used via Snakemake")


write_report(
    pred_cds=str(snakemake.input.pred_cds),
    tag_ending_cds=str(snakemake.input.tag_ending_cds),
    pot_pyl_cds=str(snakemake.input.pot_pyl_cds),
    final_cds=str(snakemake.input.final_cds),
    report=str(snakemake.output.report))
